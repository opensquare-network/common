{
  "name": "@osn/common",
  "version": "0.0.0",
  "description": "Common utils",
  "publishConfig": {
    "access": "public"
  },
  "author": "OpenSquare <https://www.opensquare.network/>",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/*"
    },
    "./es": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./es/*": {
      "import": "./dist/esm/*",
      "require": "./dist/*"
    }
  },
  "scripts": {
    "build-fast": "tsup",
    "build": "pnpm run build-fast",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@osn/constants": "^1.1.2",
    "@osn/provider-options": "^1.0.2",
    "bignumber.js": "^9.1.0",
    "lodash.debounce": "^4.0.8"
  },
  "peerDependencies": {
    "@polkadot/extension-dapp": "^0.44.2-3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@2nthony/prettier-config": "1.0.1",
    "@polkadot/extension-dapp": "^0.44.2-3",
    "@types/react": "^18.0.17",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "6.0.1",
    "typescript": "^4.7.4",
    "vitest": "0.12.6"
  }
}
